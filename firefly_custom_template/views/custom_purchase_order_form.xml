<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="custom_purchase_order_form_inherit" model="ir.ui.view">
        <field name="name">custom.purchase.order.form.inherit</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase_requisition.purchase_order_form_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='requisition_id']" position="after">
                <field name="quality_tag_ids" widget="many2many_tags" options="{'no_create': True, 'no_create_edit': True}"/>
            </xpath>
        </field>
    </record>
</odoo>